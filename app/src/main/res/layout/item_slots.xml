<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/txt_street_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Street"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_street_value"
        android:textStyle="bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txt_street_label"
        app:layout_constraintTop_toBottomOf="@+id/txt_street_label"
        tools:layout_editor_absoluteY="17dp" />

    <TextView
        android:id="@+id/txt_area_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Area"
        app:layout_constraintStart_toStartOf="@+id/txt_street_label"
        app:layout_constraintTop_toBottomOf="@+id/txt_street_value"
        tools:layout_editor_absoluteX="23dp" />

    <TextView
        android:id="@+id/txt_area_value"
        android:textStyle="bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txt_area_label"
        app:layout_constraintTop_toBottomOf="@+id/txt_area_label"
        app:layout_editor_absoluteX="17dp" />

    <TextView
        android:id="@+id/txt_city_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="City"
        app:layout_constraintStart_toStartOf="@+id/txt_area_label"
        app:layout_constraintTop_toBottomOf="@+id/txt_area_value"
        tools:layout_editor_absoluteX="23dp" />

    <TextView
        android:id="@+id/txt_city_value"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txt_city_label"
        app:layout_constraintTop_toBottomOf="@+id/txt_city_label" />

    <TextView
        android:id="@+id/txt_country_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Country"
        app:layout_constraintStart_toStartOf="@+id/txt_city_label"
        app:layout_constraintTop_toBottomOf="@+id/txt_city_value"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/txt_country_value"
        android:textStyle="bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txt_country_label"
        app:layout_constraintTop_toBottomOf="@+id/txt_country_label" />

    <TextView
        android:id="@+id/txt_status_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Status"
        app:layout_constraintStart_toStartOf="@+id/txt_country_label"
        app:layout_constraintTop_toBottomOf="@+id/txt_country_value"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="159dp" />

    <TextView
        android:id="@+id/txt_status_value"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txt_status_label"
        app:layout_constraintTop_toBottomOf="@+id/txt_status_label" />

    <TextView
        android:id="@+id/txt_busy_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Busy"
        app:layout_constraintStart_toStartOf="@+id/txt_status_label"
        app:layout_constraintTop_toBottomOf="@+id/txt_status_value"
        tools:layout_editor_absoluteX="28dp" />

    <TextView
        android:id="@+id/txt_busy_value"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txt_busy_label"
        app:layout_constraintTop_toBottomOf="@+id/txt_busy_label" />

</androidx.constraintlayout.widget.ConstraintLayout>


